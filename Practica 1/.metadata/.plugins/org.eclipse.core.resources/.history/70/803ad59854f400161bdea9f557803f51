
public class Compilador {

	public static void main(String[] args) {
		boolean s;
		double p=2.5;
		double[] d;
		d=null;
		d= new double[2];
		d[1]=2.4;
		d[0]=5.1;
		String ss= "Hola";
		
	
		Mercancia m1 = new Mercancia(p,d);
		
		m1.almacena(ss);
		s=m1.iguales(m1);
		System.out.println(s);
		
		
		
		
	}

}
