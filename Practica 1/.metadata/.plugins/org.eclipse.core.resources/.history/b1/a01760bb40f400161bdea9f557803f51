
public class Compilador {

	public static void main(String[] args) {
		boolean s;
		double p=2.4;
		double[] d;
		d=null;
		d= new double[2];
		d[1]=7.4332;
		d[0]=4.5332;
		String ss= "Hola";
		
	
		Mercancia m1 = new Mercancia(p,d);
		for (int i = 0; i < 2; i++) {
			s=m1.almacena(ss);
			System.out.println(s);
		}
		
		
	}

}
